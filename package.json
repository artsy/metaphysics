{
  "name": "metaphysics",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "repository": "https://github.com/artsy/metaphysics",
  "engines": {
    "node": "10",
    "npm": "5.6.0",
    "yarn": "1.*"
  },
  "scripts": {
    "build:fixtures": "cp src/lib/*.json build/src/lib/",
    "build:index": "babel index.js -s inline -d build",
    "build:lib": "babel src --copy-files --extensions '.ts,.js' --ignore src/test,src/integration,src/**/__tests__ -s inline -d build/src",
    "build:remote-schemas": "cp src/data/*.graphql build/src/data/",
    "build": "yarn build:lib && yarn build:index && yarn build:fixtures && yarn build:remote-schemas",
    "ci": "yarn test && yarn danger ci",
    "dev": "DEBUG=info,warn,error nf start --procfile Procfile.dev -w",
    "dump-remote-schema": "babel-node --extensions '.ts,.js' ./scripts/dump-remote-schema.ts",
    "dump-schema": "babel-node --extensions '.ts,.js' ./scripts/dump-schema.ts",
    "dump:local": "yarn dump-schema _schema.graphql",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint . --fix --ext ts",
    "prepare": "patch-package",
    "prettier-project": "prettier --write 'src/**/*.{js,ts,tsx,md,graphql}'",
    "start": "yarn run dev",
    "test:validQueries": "babel-node --extensions '.ts,.js' src/integration/__tests__/runStoredQueryTests.ts",
    "test": "yarn type-check && yarn lint && jest",
    "type-check": "tsc --noEmit --pretty",
    "verbose-dev": "DEBUG=verbose,info,error nf start --procfile Procfile.dev",
    "watch": "jest --watch"
  },
  "author": "Art.sy Inc",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-stage-3": "7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@heroku/foreman": "2.0.2",
    "accounting": "0.4.1",
    "apollo-link": "1.2.1",
    "apollo-link-context": "1.0.8",
    "apollo-link-http": "1.5.4",
    "appmetrics": "4.0.0",
    "artsy-morgan": "git://github.com/artsy/artsy-morgan.git",
    "artsy-xapp": "1.0.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-module-resolver": "3.1.1",
    "basic-auth": "1.1.0",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "dataloader": "1.3.0",
    "dd-trace": "0.7.3",
    "debug": "2.6.9",
    "express": "4.16.2",
    "express-force-ssl": "0.3.2",
    "express-graphql": "0.6.11",
    "express-interceptor": "1.2.0",
    "express-ipfilter": "0.3.1",
    "express-rate-limit": "3.2.0",
    "graphiql": "0.11.11",
    "graphql": "0.13.2",
    "graphql-crunch": "1.1.2",
    "graphql-depth-limit": "1.1.0",
    "graphql-middleware": "1.2.6",
    "graphql-relay": "0.5.4",
    "graphql-tools": "3.1.1",
    "graphql-type-json": "0.1.4",
    "heapdump": "0.3.9",
    "hot-shots": "5.6.1",
    "http-shutdown": "1.2.0",
    "i": "0.3.6",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jwt-simple": "0.5.1",
    "lodash": "4.17.11",
    "longjohn": "0.2.12",
    "marked": "0.3.9",
    "memcached": "2.2.2",
    "moment": "2.19.3",
    "moment-timezone": "0.5.25",
    "node-fetch": "1.7.3",
    "numeral": "1.5.6",
    "opentracing": "0.14.1",
    "patch-package": "5.1.2",
    "performance-now": "2.1.0",
    "postinstall-prepare": "1.0.1",
    "qs": "5.2.1",
    "rate-limit-memcached": "0.6.0",
    "raven": "2.4.2",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "relay-cursor-paging": "0.2.0",
    "request": "2.83.0",
    "source-map-support": "0.4.18",
    "url-join": "4.0.0",
    "uuid": "3.1.0"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "@types/graphql": "0.13.4"
  },
  "devDependencies": {
    "@artsy/express-reloadable": "1.4.5",
    "@types/dd-trace": "0.6.0",
    "@types/express-graphql": "0.6.1",
    "@types/express-rate-limit": "2.9.3",
    "@types/graphql": "0.13.4",
    "@types/graphql-relay": "0.4.9",
    "@types/invariant": "2.2.29",
    "@types/jest": "23.3.2",
    "@types/lodash": "4.14.86",
    "@types/moment": "2.13.0",
    "@types/moment-timezone": "0.5.2",
    "@types/node": "8.0.53",
    "@types/node-fetch": "2.1.7",
    "@types/qs": "6.5.1",
    "@types/request": "2.0.8",
    "@typescript-eslint/eslint-plugin": "1.1.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "danger": "7.0.14",
    "deep-equal": "1.0.1",
    "dotenv": "5.0.1",
    "eslint": "5.12.1",
    "eslint-config-react-app": "3.0.6",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.15.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.12.4",
    "expect.js": "0.3.1",
    "husky": "1.0.0",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "sinon": "1.17.7",
    "superagent": "3.8.3",
    "supertest": "3.1.0",
    "typescript": "3.2.4",
    "typescript-babel-jest": "1.0.5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/helper.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/src/test/helper.js",
      "/src/test/utils.js",
      "/src/test/gql.js",
      "/src/test/__mocks__"
    ],
    "transform": {
      "^.+\\.(js|ts)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testRegex": "(.test)\\.(js|ts)$"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "lint-staged": {
    "*.@(json|md|ts|graphql)": [
      "yarn prettier --write",
      "git add"
    ],
    "*.@(js)": [
      "eslint --fix",
      "yarn prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged; yarn dump-schema _schema.graphql; git add _schema.graphql",
      "pre-push": "yarn run type-check"
    }
  }
}
